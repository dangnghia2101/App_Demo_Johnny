{
  "name": "officemanager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release-dev": "react-native run-android --variant=devrelease",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "prod:setup": "ENV=production ./envscript.sh",
    "dev:setup": "ENV=development ./envscript.sh",
    "android:setup-dev": "yarn dev:setup && ENVFILE=configs/development/.env",
    "android:setup-prod": "yarn prod:setup && ENVFILE=configs/production/.env",
    "dev:android": "yarn android:setup-dev && yarn android",
    "dev:android-release": "yarn android:setup-dev && yarn android:release-dev",
    "dev:apk": "yarn dev:setup && cd android && ENVFILE=configs/development/.env ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prod:apk": "yarn prod:setup && cd android && ENVFILE=configs/production/.env ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prod:aab": "yarn prod:setup && cd android && ENVFILE=configs/production/.env ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "prod:android": "yarn prod:setup && ENVFILE=configs/production/.env npx react-native run-android",
    "prod:android-release": "yarn android:setup-prod && yarn android:release",
    "dev:ios": "yarn dev:setup && npx yarn ios --scheme 'Staging_OfficeManager'",
    "prod:ios": "yarn prod:setup && npx yarn ios --scheme 'OfficeManager'",
    "clean_build": "rm -rf ios/build android/app/build",
    "pod": "cd ios && pod install && cd ..",
    "pod:reinstall": "(rm -rf ios/Pods; rm -rf ios/Podfile.lock) && yarn pod",
    "cleanAll": "watchman watch-del-all && rm -rf yarn.lock package-lock.json node_modules ios/Pods ios/Podfile.lock android/app/build && yarn install && cd ios && pod update && cd .. && yarn start -- --reset-cache",
    "cleanAndroid": "rm -rf android/app/build && cd android && ./gradlew clean && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codepush-prod-android": "appcenter codepush release-react -a FPOLY/Office-Manager -d Production",
    "codepush-prod-ios": "appcenter codepush release-react -a FPOLY/Office-Manager-IOS -d Production",
    "codepush-staging-android": "appcenter codepush release-react -a FPOLY/Office-Manager -d Staging",
    "codepush-staging-ios": "appcenter codepush release-react -a FPOLY/Office-Manager-IOS -d Staging"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.5",
    "@gorhom/portal": "^1.0.14",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-firebase/app": "^15.4.0",
    "@react-native-firebase/auth": "^15.4.0",
    "@react-native-firebase/crashlytics": "^15.4.0",
    "@react-native-firebase/messaging": "^15.4.0",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.12",
    "@reduxjs/toolkit": "^1.9.2",
    "@types/react-native-push-notification": "^8.1.1",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "async-mutex": "^0.4.0",
    "lodash": "^4.17.21",
    "lodash.keys": "^4.2.0",
    "lottie-react-native": "^5.1.5",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-code-push": "^7.0.5",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.3.0",
    "react-native-dropdown-picker": "^5.4.4",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-picker": "^5.3.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-modalize": "^2.1.1",
    "react-native-pager-view": "^6.1.4",
    "react-native-permissions": "^3.6.1",
    "react-native-portalize": "^1.0.7",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^2.14.4",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-simple-toast": "^1.1.4",
    "react-native-size-matters": "^0.4.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.8.0",
    "react-native-tab-view": "^3.4.1",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.191",
    "@types/lodash.keys": "^4.2.7",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro": "^0.75.0",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
